{{#runs}}

  <details><summary>{{summary}}</summary>

    {{#suites}}
      *
      {{summary}}
      {{#tests}}
        *
        {{summary}}
        {{#if annotation}}
          {{#if annotation.message}}
            {{indent annotation.message}}
          {{/if}}
          {{#if annotation.raw_details}}
            {{indent annotation.raw_details}}
          {{/if}}
        {{/if}}
      {{/tests}}
    {{/suites}}

  </details>

{{/runs}}